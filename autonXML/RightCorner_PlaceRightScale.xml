<?xml version="1.0"?>
<!DOCTYPE auton SYSTEM "auton.dtd">
<auton>
	<!-- Drive to blue dot -->
	<primitive id="2"
		       liftheight="1"
			   heading="0.0"
			   distance="94.0"
			   drivespeed="150.0"
			   enddrivespeed="140"/>
	<primitive id="2"
			liftheight="2"
			heading="0.0"
			distance="100.0"
			drivespeed="140.0"
			enddrivespeed="50"/>
	<!-- Arc left -->
	<primitive id="2"
			   liftheight="2"
			   distance="42.0"
			   heading="-40.0"
			   drivespeed="50.0"/>
	<!-- Hold position and release cube -->
	<primitive id="0"
				liftheight="2"
				time="0.25"
				cubespeed="-0.75" />
	<!-- Turn left to face cube and lower elevator -->
	<primitive  id="6"
				cubespeed="-0.1"
				liftheight="0"
				heading="-110"
						/>
	<!-- Drive To Cube -->
	<primitive  id="9"
				heading="0.0"
				liftheight="0"
				cubespeed="1.0"
				drivespeed="60.0"/>
	<primitive id="0"
				time="0.25"
				cubespeed="1" />
	<!-- Drive Distance -->
	<primitive  id="2"
				heading="0.0"
				distance="20.0"
				cubespeed="0"
				liftheight="1"
				drivespeed="-30.0"/>
	<!-- Turn right -->
	<primitive  id="6"
				liftheight="2"
				heading="138"/>
	<!-- Do nothing and raise elevator -->
	<primitive id="0"
				time="0.1"
				liftheight="2"/>
	<!-- Drive Distance with slight arc left -->
	<primitive  id="2"	
				heading="-3.0"
				liftheight="2"
				distance="20.0"
				cubespeed="0"
				drivespeed="45.0"/>
	<!-- shoot -->
	<primitive  id="0"
				time="0.5"
				cubespeed="-0.5" />
	<!-- backup -->
	<primitive  id="2"
				heading="0.0"
				distance="-30.0"
				cubespeed="-0.5"
				drivespeed="-30.0"/>
</auton>